<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/default_rtl_action_bar_selector_pressed" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/default_rtl_action_bar_selector_focused" />
        </shape>
    </item>
    <item android:drawable="@color/default_rtl_action_bar_selector_normal" />
</selector>